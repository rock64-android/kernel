	};
	/* rk new remote */
	ir_key6 {
		rockchip,usercode = <0xfe01>;
		rockchip,key_table =
		<0xbf  KEY_POWER>,
		<0xbe  KEY_MUTE>,
		<0xec  KEY_REPLY>,
		<0xef  KEY_VOLUMEDOWN>,
		<0xe7  KEY_VOLUMEUP>,
		<0xee  KEY_HOME>,
		<0xe6  KEY_BACK>,
		<0xe9  KEY_UP>,
		<0xe5  KEY_DOWN>,
		<0xae  KEY_LEFT>,
		<0xaf  KEY_RIGHT>,
		<0xb3  KEY_MENU>,
		<0xff  388>,//mouse
		<0xb1  KEY_1>,
		<0xf2  KEY_2>,
		<0xf3  KEY_3>,
		<0xb5  KEY_4>,
		<0xf6  KEY_5>,
		<0xf7  KEY_6>,
		<0xb9  KEY_7>,
		<0xfa  KEY_8>,
		<0xfb  KEY_9>,
		<0xfe  KEY_0>,
		<0xaa  KEY_F1>, //<<
		<0xa5  KEY_F2>, //>||
		<0xad  KEY_F3>,//KEY_FILE_EXPLORE
		<0xab  KEY_F4>,//KEY_MEDIA_PREVIOUS
		<0xbc  KEY_F5>,//KEY_MEDIA_NEXT
		<0xf0  KEY_F6>,//KEY_ALLAPP
