/dts-v1/;

#include "rk3328.dtsi"
#include "rk3328-evb.dtsi"
#include "rk-stb-keymap.dtsi"

/ {
	io-domains {
		vccio4-supply = <&rk805_dcdc4_reg>;
	};

	leds {
		compatible = "gpio-leds";
		power-green {
		        gpios = <&rk805 0 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "on";
			mode = <0x23>;
		};

		net-green {
			gpios = <&rk805 1 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "off";
			mode = <0x05>;
		};

		ir {
			/* gpios = <&gpio2 GPIO_C2 GPIO_ACTIVE_HIGH>; */
			linux,default-trigger = "ir";
			default-state = "off";
			mode = <0x00>;
		};
	};
	wireless-wlan {
            compatible = "wlan-platdata";
            wifi_chip_type = "rtl8189es";
                 sdio_vref = <1800>;
                  WIFI,poweren_gpio = <&gpio1 GPIO_C2 GPIO_ACTIVE_HIGH>;
                  WIFI,host_wake_irq = <&gpio1 GPIO_C3 GPIO_ACTIVE_HIGH>;
                  status = "okay";
	};

	wireless-bluetooth {
		status = "disabled";
	};
};

&clk_core_dvfs_table {
	operating-points = <
		/* KHz	uV */
		408000	950000
		600000	950000
		816000	1000000
		1008000	1100000
		1200000	1225000
		1296000	1300000
		1392000	1350000
		1512000	1350000
		>;
	status = "okay";
};

&sdio {
	status = "okay";
};

&spdif {
	pinctrl-0 = <&spdifm0_tx>;
	status = "okay";
};
&usb2_otg {
	/* 0 - Normal, 1 - Force Host, 2 - Force Device */
	rockchip,usb-mode = <1>;
};
&pwm3 {
	/* for IPTV */
	ir_key2{
		rockchip,usercode = <0xff00>;
		rockchip,key_table =
		<0xa8  KEY_POWER>,
		<0xa4  KEY_MUTE>,
		<0xfd  KEY_REPLY>,
		<0xf0  KEY_VOLUMEDOWN>,
		<0xb3  KEY_VOLUMEUP>,
		<0xb0  KEY_HOME>,
		<0xe5  KEY_BACK>,
		<0xbc  KEY_UP>,
		<0xf5  KEY_DOWN>,
		<0XF9  KEY_LEFT>,
		<0XF1  KEY_RIGHT>,
		<0XB1  KEY_MENU>,
		<0XB2  388>,//mouse
		<0XEF  KEY_1>,
		<0XEE  KEY_2>,
		<0XED  KEY_3>,
		<0XEC  KEY_4>,
		<0XEB  KEY_5>,
		<0XEA  KEY_6>,
		<0XE8  KEY_7>,
		<0XE7  KEY_8>,
		<0XE6  KEY_9>,
		<0XE2  KEY_0>,
		<0xa5  KEY_F1>, //media center
		<0xad  KEY_F2>, //KEY_BROSE
		<0xaf  KEY_F3>,//KEY_FILE_EXPLORE
		<0xa7  KEY_F4>,//KEY_MEDIA_PREVIOUS
		<0xe4  KEY_F5>,//KEY_MEDIA_NEXT
		<0XE3  KEY_F6>,//KEY_ALLAPP
